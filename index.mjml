        </mj-column>
      </mj-section>

      <!-- REUSE -->
      <mj-section>
        <mj-group css-class="hide-mobile">
          <mj-column vertical-align="middle" padding="0">
            <mj-image padding="0" mj-class="rrr-image" src="https://pentadome.github.io/r3stone-email-1/images/16_reuse_icon.png" />
          </mj-column>
          <mj-column vertical-align="middle">
            <mj-text mj-class="rrr-header"> Reuse </mj-text>
            <mj-text padding-bottom="16px" mj-class="rrr-text"> 100% losmaakbaar </mj-text>
            <mj-text padding-bottom="16px" mj-class="rrr-text"> Hergebruik zonder enig kwaliteitsverlies </mj-text>
            <mj-text mj-class="rrr-text">Geen inzet van speciale apparatuur voor montage en/of demontage </mj-text>
          </mj-column>
        </mj-group>
      </mj-section>

      <mj-section css-class="hide-desktop" padding="0">
        <mj-group>
          <mj-column vertical-align="middle">
            <mj-text mj-class="rrr-header"> Reuse </mj-text>
          </mj-column>
          <mj-column  vertical-align="middle" padding="0">
            <mj-image padding="0" mj-class="rrr-image-phone" src="https://pentadome.github.io/r3stone-email-1/images/16_reuse_icon.png" />
          </mj-column>
        </mj-group>
      </mj-section>
      <mj-section css-class="hide-desktop" padding="0">
        <mj-column>
            <mj-text padding-bottom="16px" mj-class="rrr-text"> 100% losmaakbaar </mj-text>
            <mj-text padding-bottom="16px" mj-class="rrr-text"> Hergebruik zonder enig kwaliteitsverlies </mj-text>
            <mj-text mj-class="rrr-text"> Geen inzet van speciale apparatuur voor montage en/of demontage </mj-text>
        </mj-column>
      </mj-section>

      <!-- RECYCLE -->
